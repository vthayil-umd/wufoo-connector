<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name>Wufoo to SysAid Connector</display-name>
  <servlet>
    <servlet-name>EntryController</servlet-name>
    <servlet-class>edu.umd.lib.wufoosysaid.EntryController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EntryController</servlet-name>
    <url-pattern>/entry/*</url-pattern>
  </servlet-mapping>
  <context-param>
    <param-name>xslLocation</param-name>
    <param-value>/apps/apache-tomcat-7.0.56/webapps/xsls</param-value>
  </context-param>
</web-app>

